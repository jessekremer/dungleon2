	-- x = x position
	-- y = y position
	-- pl = transparency colour
	-- hl = highlight
	-- m = match
	-- bg = background style
	local highlighter=hl

	if highlighter then
		if m=="none" then
			spr(168,x-2,y-2)
			spr(169,x+6,y-2)
			spr(184,x-2,y+6)
			spr(185,x+6,y+6)
		elseif m=="wrong" then
			spr(170,x-2,y-2)
			spr(171,x+6,y-2)
			spr(186,x-2,y+6)
			spr(187,x+6,y+6)
		elseif m=="close" then
